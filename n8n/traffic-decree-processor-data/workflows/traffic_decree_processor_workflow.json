        "query": "INSERT INTO verkeersbesluiten.verkeersai (\n  id,\n  gemeente,\n  titel,\n  samenvatting,\n  ai_samenvatting,\n  available,\n  original_image,\n  PATH,\n  geom_multipolygon,\n  geom_multilinestring,\n  geom_multipoint\n)\nVALUES (\n  '{{ $json.id }}',\n  '{{ $json.gemeente }}',\n  '{{ $json.titel }}',\n  '{{ $json.samenvatting }}',\n  '{{ $json['ai-samenvatting'] }}',\n  '{{ $json.beschikbaar }}',\n  '{{ $json['original-image'] }}',\n  '{{ $json['geotiff-url'] }}',\n  {{ $json.geom_multipolygon ? `ST_GeomFromText('${$json.geom_multipolygon}', 28992)` : 'NULL' }},\n  {{ $json.geom_multilinestring ? `ST_GeomFromText('${$json.geom_multilinestring}', 28992)` : 'NULL' }},\n  {{ $json.geom_multipoint ? `ST_GeomFromText('${$json.geom_multipoint}', 28992)` : 'NULL' }}\n)\nON CONFLICT (id) DO UPDATE SET\n  gemeente = EXCLUDED.gemeente,\n  titel = EXCLUDED.titel,\n  samenvatting = EXCLUDED.samenvatting,\n  ai_samenvatting = EXCLUDED.ai_samenvatting,\n  available = EXCLUDED.available,\n  original_image = EXCLUDED.original_image,\n  PATH = EXCLUDED.PATH,\n  geom_multipolygon = EXCLUDED.geom_multipolygon,\n  geom_multilinestring = EXCLUDED.geom_multilinestring,\n  geom_multipoint = EXCLUDED.geom_multipoint;",
