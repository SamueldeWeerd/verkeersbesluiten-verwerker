    {
      "parameters": {
        "jsCode": "const input = $input.first().json.body;\n\nconst startDate = new Date(input.start_date);\nconst endDate = new Date(input.end_date);\n\nconst bordcodes = input.bordcode_categories || null;\nconst provinces = input.provinces || null;\nconst gemeenten = input.gemeenten || null;\n\n// Helper to build query string from optional arrays\nfunction toQueryParam(key, value) {\n  if (!value || !Array.isArray(value) || value.length === 0) return null;\n  return `${encodeURIComponent(key)}=${encodeURIComponent(value.join(','))}`;\n}\n\nconst result = [];\n\nfor (let d = new Date(startDate); d <= endDate; d.setDate(d.getDate() + 1)) {\n  const dateStr = d.toISOString().split('T')[0];\n\n  const queryParts = [];\n\n  const bordParam = toQueryParam('bordcode_categories', bordcodes);\n  const provParam = toQueryParam('provinces', provinces);\n  const gemParam = toQueryParam('gemeenten', gemeenten);\n\n  if (bordParam) queryParts.push(bordParam);\n  if (provParam) queryParts.push(provParam);\n  if (gemParam) queryParts.push(gemParam);\n\n  const queryString = queryParts.join('&');\n\n  result.push({\n    json: {\n      date: dateStr,\n      query: queryString,\n    }\n  });\n}\n\nreturn result;"
      },
